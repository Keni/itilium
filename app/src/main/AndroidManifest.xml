<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="keni.itilium">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Optima.ViewAppOptimaActivity" android:windowSoftInputMode="stateAlwaysHidden" android:screenOrientation="portrait" />
        <activity android:name=".Optima.AddAppOptimaActivity" android:windowSoftInputMode="stateAlwaysHidden" android:screenOrientation="portrait" />
        <activity android:name=".Paritet.ViewAppParitetActivity" android:windowSoftInputMode="stateAlwaysHidden" android:screenOrientation="portrait" />
        <activity android:name=".Paritet.AddAppParitetActivity" android:windowSoftInputMode="stateAlwaysHidden" android:screenOrientation="portrait" />
    </application>

</manifest>